<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.sagarmakhija1994gmail.finomenalaunchpad.activityStage10_1"
    android:id="@+id/layoutSettings">
    <FrameLayout
        android:layout_width="400dp"
        android:layout_height="400dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/Stage4_1">


        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/button10_1_5"
            android:layout_gravity="center_horizontal|top"
            android:background="@drawable/button_green"
            android:textSize="50dp"
            android:textColor="#ffffff"/>

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/button10_1_4"
            android:layout_gravity="left|bottom"
            android:background="@drawable/button_green"
            android:textSize="50dp"
            android:textColor="#ffffff"/>

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/button10_1_3"
            android:layout_gravity="right|bottom"
            android:background="@drawable/button_green"
            android:textSize="50dp"
            android:textColor="#ffffff"/>



        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/button10_1_7"
            android:layout_gravity="left|top"
            android:background="@drawable/button_green"
            android:textSize="50dp"
            android:textColor="#ffffff"/>

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/button10_1_8"
            android:layout_gravity="right|top"
            android:background="@drawable/button_green"
            android:textSize="50dp"
            android:textColor="#ffffff"/>

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:text=""
            android:id="@+id/button10_1_6"
            android:layout_gravity="center_horizontal|bottom"
            android:background="@drawable/button_green"
            android:textSize="50dp"
            android:textColor="#ffffff"/>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">
            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:text=""
                android:id="@+id/button10_1_1"
                android:layout_gravity="right|center_vertical"
                android:background="@drawable/button_green"
                android:textSize="50dp"
                android:layout_marginBottom="10dp"
                android:textColor="#ffffff"/>

            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:text=""
                android:id="@+id/button10_1_2"
                android:layout_gravity="left|center_vertical"
                android:background="@drawable/button_green"
                android:textSize="50dp"
                android:layout_marginTop="10dp"
                android:textColor="#ffffff"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left|center">
            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:text=""
                android:id="@+id/button10_1_9"
                android:layout_gravity="right|center_vertical"
                android:background="@drawable/button_green"
                android:textSize="50dp"
                android:layout_marginRight="10dp"
                android:textColor="#ffffff"/>

            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:text=""
                android:id="@+id/button10_1_10"
                android:layout_gravity="left|center_vertical"
                android:background="@drawable/button_green"
                android:textSize="50dp"
                android:layout_marginLeft="10dp"
                android:textColor="#ffffff"/>

        </LinearLayout>

    </FrameLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stage-4 Level-1"
        android:id="@+id/textView5"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="25dp"
        android:textColor="#ffffff"/>
</RelativeLayout>
